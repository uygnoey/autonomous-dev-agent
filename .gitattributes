# Git LFS 설정 - 대용량 테스트 파일
# QC 테스트 케이스 및 결과 파일 (JSONL)
tests/qc/**/test_cases.jsonl filter=lfs diff=lfs merge=lfs -text
tests/qc/**/results.jsonl filter=lfs diff=lfs merge=lfs -text
# QA 테스트 케이스 및 결과 파일 (JSONL)
tests/qa/test_cases.jsonl filter=lfs diff=lfs merge=lfs -text
tests/qa/results.jsonl filter=lfs diff=lfs merge=lfs -text
# E2E 테스트 결과 파일
tests/e2e/**/*.jsonl filter=lfs diff=lfs merge=lfs -text
tests/qa/*.jsonl filter=lfs diff=lfs merge=lfs -text
